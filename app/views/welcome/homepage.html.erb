    <!-- Contact Section> -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Contactanos</h2>
                    <h3 class="section-subheading text-muted">Cualquier duda o informacion no dude en contactarnos <br> Tel: +52 (55) 53535676 , 53535192 , 85013695</br> </h3>
                </div>
            </div>
            <div class="row">
                <div class ="col-md-6">
                    <%= form_for @contact_request, html: {role: "form", id: "contact-form"} do |f| %>
                        <div class ="form-group">
                            <label>
                                Nombre *
                            </label>
                            <div class="controls">
                                <%= f.text_field :name, :class=> 'form-control' %>
                                <small class="help-block"> </small>
                            </div>
                        </div>
                        <div class ="form-group">
                            <label>
                                Email *
                            </label>
                            <div class="controls">
                                <%= f.text_field :email, :class=> 'form-control' %>
                                <small class="help-block"> </small>
                            </div>
                        </div>
                        <div class ="form-group">
                            <label>
                                Teléfono 
                            </label>
                            <div class="controls">
                                <%= f.text_field :phone_number, :class=> 'form-control' %>
                                <small class="help-block"> </small>
                            </div>
                        </div>
                        <div class ="form-group">
                            <label>
                                Mensaje * 
                            </label>
                            <div class="controls">
                                <%= f.text_area :message, :class=> 'form-control', rows: 5 %>
                                <small class="help-block"> </small>
                            </div>
                        </div>
                        <div class ="form-actions text-right">
                            <%= f.submit "Enviar", :class => 'btn btn-lg' %>
                        </div>
                    <%end%>
                </div>
            </div>
        </div>
    </section> 

